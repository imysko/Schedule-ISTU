<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_timetable">

    <fragment
        android:id="@+id/navigation_timetable"
        android:name="com.example.timetable.ui.timetable.TimeTableFragment"
        android:label="@string/title_timetable"
        tools:layout="@layout/fragment_timetable" />

    <fragment
        android:id="@+id/navigation_exams"
        android:name="com.example.timetable.ui.exams.ExamsFragment"
        android:label="@string/title_exams"
        tools:layout="@layout/fragment_exams" />

    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.example.timetable.ui.settings.SettingsFragment"
        android:label="@string/title_settings"
        tools:layout="@layout/fragment_settings" />
</navigation>